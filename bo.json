  "friday_creative_project_activity": "གསར་གཏོད་ཀྱི་ལས་ཀ།",
  "friday_creative_project_desc": "ཆུ་བདག་གཉེར་དང་། སྲོག་ཆགས་ཀྱི་བདེ་དོན། རང་བྱུང་ཁམས་ཀྱི་ངོ་མཚར། (དཔེར་ན་མེ་རི་འབར་བ་ལྟ་བུ།) སོགས་ཀྱི་བརྗོད་གཞི་ལ་དམིགས་པའི་མཉམ་ལས་ཀྱི་ལས་ཀ།",
  "friday_debate_discussion_activity": "བགྲོ་གླེང་དང་གྲོས་བསྡུར།",
  "friday_debate_discussion_desc": "ཤེས་ཡོན་གྱི་ཤུགས་རྐྱེན་དང་། འགྱིག་དངོས་བེད་སྤྱོད། གྲོང་གསེབ་དང་གྲོང་ཁྱེར་གྱི་འཚོ་བ། དེ་བཞིན་དྲ་རྒྱ་བེད་སྤྱོད་ཀྱི་ཡོན་ཏན་དང་སྐྱོན་ཆ་ལྟ་བུའི་བརྗོད་གཞི་མང་པོ་ལ་དམིགས་པའི་བགྲོ་གླེང་ཞིབ་ཚགས།",
  "future_updates_heading": "ཆ་འཕྲིན་གཞན་ཡང་མྱུར་དུ་འབྱོར་རྒྱུ་ཡིན།",
  "future_updates_paragraph_1": "ང་ཚོས་ང་ཚོའི་དྲ་རྒྱ་རྒྱ་སྐྱེད་གཏོང་ཆེད་མུ་མཐུད་ལས་ཀ་བྱེད་བཞིན་ཡོད་ལ། དེའི་ནང་དུ་ང་ཚོའི་ལས་རིམ་དང་། དགེ་རྒན། སློབ་གཉེར་ཁང་གི་སྒྲིག་ཆས། སློབ་མའི་འཚོ་བ་བཅས་ཀྱི་སྐོར་ལ་ཞིབ་ཕྲའི་ཆ་འཕྲིན་མང་པོ་ཚུད་ཡོད། དུས་རྒྱུན་ཞིབ་བཤེར་བྱེད་པར་ཕེབས་རོགས་དང་ཐར་ལམ་སློབ་གྲྭ་ནས་གསར་འགྱུར་ཆེས་གསར་ཤོས་ཤེས་རྟོགས་བྱེད་པར་ཕེབས་རོགས།",
  "future_updates_paragraph_2": "ཁྱེད་ཀྱི་བཟོད་སྒོམ་དང་ང་ཚོའི་སློབ་གཉེར་ཁང་ལ་དོ་སྣང་བྱེད་པར་བཀའ་དྲིན་ཆེ་ཞུ།",
  "friday_activities_title": "བདུན་རེའི་པ་སངས་ཉིན་གྱི་བྱེད་སྒོ།",
  "friday_activities_intro": "ཐར་ལམ་སློབ་གྲྭའི་པ་སངས་ཉིན་རེ་བཞིན་ལུས་རྩལ་ཕྱོགས་ཀྱི་བྱེད་སྒོ་སྣ་ཚོགས་ལ་དམིགས་པ་ཡིན་ལ། དེས་རྒྱུན་རེའི་སློབ་གཉེར་དང་ཆོས་ཀྱི་ལག་ལེན་ལས་གཞན་གསར་གཏོད་ཀྱི་བསམ་བློ་དང་། དཔྱད་ཞིབ་ཀྱི་བསམ་བློ། སྤྱི་ཚོགས་ཀྱི་ནུས་པ་བཅས་སྐྱེད་སྲིང་བྱེད་ཀྱི་ཡོད།"
}