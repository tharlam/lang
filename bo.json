  "modal_guru_yoga": "བླ་མའི་རྣལ་འབྱོར།",
  "modal_buddha_events": "སངས་རྒྱས་ཀྱི་དུས་སྟོན་གྱི་མཛད་སྒོ། (དཔེར་ན་ས་ག་ཟླ་བ། ལྷ་བབས་དུས་ཆེན།)",
  "what_we_do_image_alt": "སློབ་མ་སློབ་སྦྱོང་བྱེད་བཞིན་པ།",
  "what_we_do_heading": "ང་ཚོས་གང་བྱེད་བཞིན་ཡོད།",
  "what_we_do_paragraph_1": "ཐར་ལམ་སློབ་གྲྭ་ནས་ནང་ཆོས་སྲོལ་རྒྱུན་གྱི་སློབ་གཉེར་དང་དེང་རབས་ཀྱི་ཤེས་ཡོན་གཉིས་ཀ་ཚུད་པའི་ཤེས་ཡོན་གྱི་ཉམས་མྱོང་ཁྱོན་ཡོངས་ནས་སྤྲོད་ཀྱི་ཡོད། ང་ཚོའི་ལས་རིམ་ཁག་ནི་ཤེས་རིག་གི་འཕེལ་རྒྱས་དང་། ཀུན་སྤྱོད་ཀྱི་འཕེལ་རྒྱས། དེ་བཞིན་ལག་ལེན་གྱི་ནུས་པ་བཅས་སྐྱེད་སྲིང་བྱེད་ཆེད་དུ་ཇུས་འགོད་བྱས་ཡོད།",
  "what_we_do_paragraph_2": "ང་ཚོས་དཔྱད་ཞིབ་ཀྱི་བསམ་བློ་དང་གསར་གཏོད། དེ་བཞིན་བརྗོད་གཞི་མང་པོ་ལ་ཟབ་གནས་ཀྱི་གོ་བ་ཡོང་བར་སྐུལ་འདེད་བྱེད་པའི་སློབ་སྦྱོང་གི་ཁོར་ཡུག་ཕུན་སུམ་ཚོགས་པ་ཞིག་སྤྲོད་ཀྱི་ཡོད་ལ། དེས་སློབ་མ་ཚོ་འབྱུང་འགྱུར་གྱི་དཀའ་ངལ་དང་གོ་སྐབས་ལ་གྲ་སྒྲིག་བྱེད་ཀྱི་ཡོད།",
  "history_tagline": "ང་ཚོའི་རྒྱུན་འཛིན།",
  "history_main_heading": "དད་པ་དང་སློབ་གཉེར་གྱི་དུས་རབས་བརྒྱུད་པའི་འགྲུལ་བཞུད།",
  "history_from_ancient_heading": "གནའ་བོའི་རྩ་བ་ནས་མདུན་ལམ་གསར་པའི་བར།",
  "history_paragraph_1": "ཐར་ལམ་དགོན་པ་ནི། ས་སྐྱ་ནང་བསྟན་གྱི་བཙུན་ལག་བརྩི་བཀུར་ཅན་ཞིག་ཡིན་ཞིང་། དེའི་མིང་ཆ་ཚང་ལ་**ཤྲི་ཐར་ལམ་ས་བཟང་རྣམ་རྒྱལ་གླིང་དགོན་པ་**ཞེས་བྱ་བ་ཡིན། དེ་ནི་**སྤྱི་ལོ་༡༤༣༦**ལོར་**ཁམས་ཤར་ཕྱོགས་བོད་**དུ་**དགའ་རབ་འབྱམས་པ་ཀུན་དགའ་ཡེ་ཤེས་**ཀྱིས་ཐོག་མར་བཙུགས་པ་ཡིན་ལ། ནང་ཆོས་ཀྱི་བསླབ་བྱ་དང་ལག་ལེན་གྱི་ལྟེ་གནས་ཤིག་ཏུ་ལོ་རྒྱུས་རིང་མོ་དང་གལ་ཆེན་ཞིག་གི་འགྲུལ་བཞུད་དབུ་འཛུགས་གནང་བ་ཡིན།",
  "history_paragraph_2": "དུས་རབས་མང་པོའི་རྗེས་སུ་དེའི་རྒྱུན་འཛིན་ལ་བསྐྱར་བཞེངས་གནང་བ་ཡིན། **སྤྱི་ལོ་༡༩༨༡**ལོར་**བལ་ཡུལ་གྱི་ཀ་ཐ་མན་དུའི་བྷོ་དྷ་ནཱ་ཐ་**རུ་**འདས་ཞུས་རིན་པོ་ཆེ་**མཆོག་གིས་བསྐྱར་དུ་བཙུགས་གནང་བ་ཡིན་ལ། དེས་ཐར་ལམ་དགོན་པའི་རྒྱུན་འཛིན་རིང་མོ་མུ་མཐུད་བྱེད་པ་དང་། འབྱུང་འགྱུར་གྱི་མི་རབས་ལ་ཆོས་དང་དངོས་པོའི་རྒྱུན་མི་ཆད་པ་ཡོང་བར་བྱས་ཡོད།",
  "monastery_image_alt": "ཐར་ལམ་དགོན་པའི་ལོ་རྒྱུས་ཀྱི་པར་རིས།",
  "history_academy_bridging_heading": "ཐར་ལམ་སློབ་གྲྭ། འཛམ་གླིང་གཉིས་འབྲེལ་མཁན།",
  "history_academy_paragraph_1": "ཐར་ལམ་སློབ་གྲྭ་ནི། ཐར་ལམ་དགོན་པའི་གལ་ཆེའི་ཆ་ཤས་ཤིག་ཡིན་ཞིང་། **སྤྱི་ལོ་༡༩༨༥**ལོར་དགོན་པ་འདི་གར་བཙུགས་པ་ཡིན། དེའི་གཙོ་བོའི་དམིགས་ཡུལ་ནི་ན་གཞོན་གྲྭ་པ་ཚོར་ནུབ་ཕྱོགས་ཀྱི་ཤེས་ཡོན་གལ་ཆེན་ངོ་སྤྲོད་བྱེད་པ་དེ་ཡིན་ལ། དེའི་ནང་དུ་ཨང་རྩིས་དང་དབྱིན་ཇི། ཚན་རིག སྐད་ཡིག་སྣ་ཚོགས་སོགས་ཚུད་ཡོད། དམིགས་བསལ་གྱི་ལས་འགུལ་འདི་གལ་ཆེན་ཞིག་ཡིན་ལ། དེའི་སྐབས་སུ་ཐར་ལམ་དགོན་པ་ནས་གཙོ་བོ་ཆོ་ག་ཕྱག་ལེན་དང་བོད་ཡིག་ཁོ་ན་ཁྲིད་ཀྱི་ཡོད།",
  "history_academy_paragraph_2": "སྤྱི་ལོ་༢༠༠༠ ལོའི་ཐོག་མར་དོ་གལ་ཆེ་བའི་ལེགས་བཅོས་འགོ་འཛུགས་བྱས་ཏེ། ནུབ་ཕྱོགས་ཀྱི་སློབ་ཚན་ཁྱོན་ཡོངས་ནས་ལེགས་འགྲུབ་ཡོང་བར་བྱས་ཡོད། འདས་པའི་ལོ་ཉི་ཤུའི་རིང་ང་ཚོས་སློབ་གྲྭ་ཚང་མ་ལ་སློབ་གཉེར་གྱི་སྤུས་ཚད་ཆེས་མཐོ་ཤོས་སྤྲོད་ཆེད་འབད་བརྩོན་བྱས་ཡོད། དེས་ང་ཚོའི་སློབ་མ་ཚོ་ཨང་རྩིས་དང་ཚན་རིག་སོགས་ཀྱི་བརྗོད་གཞི་ཁག་ལ་ཕུལ་བྱུང་ཡོང་བར་བྱས་པ་དང་། སྐད་ཡིག་མང་པོ་ལ་མཁས་པར་གྱུར་ཡོད། དེང་སྐབས་ཐར་ལམ་སློབ་གྲྭ་ནས་སློབ་མ་བརྒྱ་ཙམ་ལ་སློབ་ཁྲིད་བྱེད་བཞིན་ཡོད་ལ། ཁོང་ཚོ་གྲྭ་བཙུན་གྱི་འཚོ་བ་དང་འཛམ་གླིང་ཁྱོན་ཡོངས་ལ་གྲ་སྒྲིག་བྱེད་ཀྱི་ཡོད།",
