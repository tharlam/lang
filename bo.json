  "testimonial_lobsang_name": "བློ་བཟང་ཉི་མ།",
  "testimonial_lobsang_role": "ལོ་༡༢། འཛིན་གྲྭ་༤པ།",
  "testimonial_pema_text": "“བདེ་མོ་! ངའི་མིང་ལ་པདྨ་ཤེས་རབ་ཟེར། ཐར་ལམ་སློབ་གླིང་དུ་སློབ་སྦྱོང་བྱེད་རྒྱུ་བྱུང་བར་དགའ་པོ་ཆེན་པོ་བྱུང་། དགེ་རྒན་རྣམས་བྱམས་སྙིང་རྗེ་ཅན་ཡིན་ལ་། འདིར་གྲོགས་པོ་མང་པོ་ཡོད། ང་སངས་རྒྱས་ཀྱི་བསྟན་པ་སློབ་གཉེར་བྱེད་རྒྱུར་དགའ་པོ་ཡོད་ལ་། དབྱིན་ཇིའི་དེབ་ཀློག་རྒྱུར་ཡང་དགའ་པོ་ཡོད། ང་ཚོ་རྩེད་མོ་རྩེ་བ་དང་། སློབ་སྦྱོང་བྱེད་པ། གཅིག་ལ་གཅིག་རོགས་པ་བྱེད་པ་བཅས་ནི་ཁྱིམ་ཚང་ཆེན་པོ་ཞིག་དང་འདྲ་པོ་འདུག ཐར་ལམ་སློབ་གླིང་ནི་ངའི་ཁྱིམ་གཉིས་པ་རེད།”",
  "testimonial_pema_alt": "སློབ་མ་པདྨ་ཤེས་རབ་ཀྱི་འདྲ་པར།",
  "testimonial_pema_name": "པདྨ་ཤེས་རབ།",
  "testimonial_pema_role": "ལོ་༡༣། འཛིན་གྲྭ་༦པ།",
  "testimonial_dhundup_text": "“བདེ་མོ་! ང་དོན་གྲུབ་བླ་མ་ཡིན། ང་ཆུང་ཆུང་སྐབས་ཐར་ལམ་སློབ་གླིང་དུ་ཡོང་བ་ཡིན། དེ་ནས་བཟུང་། ང་གང་ཡང་མང་པོ་སྦྱངས་བྱུང་། སྒོམ་རྒྱག་པ་ནས་ཨང་རྩིས་བར་དུ་། འཛིན་གྲྭ་རྣམས་སྤྲོ་པོ་ཡོད་ལ་། དགེ་རྒན་རྣམས་ཀྱང་ང་ཚོར་བྱམས་པོ་ཡོད། ང་སློབ་གླིང་འདིའི་ཁོངས་སུ་ཡོད་པར་སྤོབས་པ་སྐྱེ་གི་འདུག ཉིན་ཞིག་ང་ལ་འདིར་རོགས་པ་བྱས་པ་ནང་བཞིན་གཞན་ལ་ཡང་རོགས་པ་བྱེད་འདོད་ཡོད།”",
  "testimonial_dhundup_alt": "སློབ་མ་དོན་གྲུབ་ཀྱི་འདྲ་པར།",
  "testimonial_dhundup_name": "དོན་གྲུབ།",
  "testimonial_dhundup_role": "ལོ་༡༢། འཛིན་གྲྭ་༤པ།",
  "testimonial_gyen_text": "“ང་ཚོར་རྒྱབ་སྐྱོར་གནང་མཁན་སྦྱིན་བདག་ཡོངས་ལ་ཐུགས་རྗེ་ཆེ་ཞུ་རྒྱུ་ཡིན། ཁོང་ཚོས་ང་ཚོར་ཤེས་བྱ་ཁོ་ན་མ་ཡིན་པར་མི་བཟང་པོ་ཞིག་ཇི་ལྟར་བྱེད་དགོས་པ་ཡང་སློབ་གསོ་གནང་གི་ཡོད།”",
  "testimonial_gyen_alt": "དགེ་རྒན་རྒན་པ་དགེ་ལེགས་བཤད་ཀྱི་འདྲ་པར།",
  "testimonial_gyen_name": "དགེ་ལེགས་བཤད།",
  "testimonial_gyen_role": "དགེ་རྒན་རྒན་པ།",
  "achievements_section_title": "ང་ཚོའི་གྲུབ་འབྲས་དང་བྱ་དགའ།",
  "achievements_section_subtitle": "ཐར་ལམ་སློབ་གླིང་དང་དེའི་སྤྱི་ཚོགས་ཀྱི་མངོན་གསལ་དོད་པའི་གྲུབ་འབྲས་དང་ངོས་འཛིན་རྣམས་ལ་བསྔགས་བརྗོད་བྱེད་པ།",
  "achievement1_title": "ནུབ་ཕྱོགས་ཀྱི་ཤེས་ཡོན་གྱི་སྔོན་གཤེད།",
  "achievement1_desc": "ཐར་ལམ་སློབ་གླིང་གི་གྲུབ་འབྲས་གལ་ཆེན་ཞིག་ནི་ནུབ་ཕྱོགས་ཀྱི་ཤེས་ཡོན་གྱི་སློབ་ཚན་ལ་བདེ་བླག་ངང་བསྟུན་རྒྱུ་བྱུང་བ་དེ་ཡིན། འདིའི་གྲུབ་འབྲས་ལས་ང་ཚོའི་ན་གཞོན་དགེ་འདུན་པ་རྣམས་ཀྱི་སློབ་གཉེར་གྱི་མཐོང་རྒྱ་རྒྱ་ཆེར་བསྐྱེད་རྒྱུ་བྱུང་སོང་། དེ་ནི་སྲོལ་རྒྱུན་གྱི་སློབ་གཉེར་དང་ཨང་རྩིས་དང་། དབྱིན་སྐད། ཚན་རིག་སོགས་ཀྱི་སློབ་ཚན་ཁག་མཉམ་དུ་བསྲེ་སྦྱོར་བྱེད་རྒྱུ་དེ་ཡིན།",
  "achievement2_title": "སློབ་ཚན་རྒྱ་བསྐྱེད་དང་དབུ་འཛིན་གསར་པ་ (༡༩༡༨)",
  "achievement2_desc": "༡༩༡༨ ལོར་སློབ་གཉེར་ཁང་གི་སློབ་ཚན་རྒྱ་བསྐྱེད་དང་། དེང་རབས་ཀྱི་སློབ་ཚན་ཁག་གསར་དུ་འཛུགས་རྒྱུ་བྱུང་བ་འདི་གྲུབ་འབྲས་གལ་ཆེན་ཞིག་ཡིན། དེའི་རྗེས་སུ་ཐར་ལམ་སློབ་གླིང་ལ་དབུ་འཛིན་གསར་པ་ཞིག་བསྐོ་བཞག་བྱེད་ཐུབ་པ་བྱུང་སྟེ་། སློབ་གླིང་གི་དབུ་འཛིན་ལྷན་ཁང་གི་ནུས་པ་གོང་འཕེལ་བཏང་ནས་། སློབ་གསོའི་ལས་དོན་ལ་དོན་གཉེར་དང་། རིག་རྩལ་གྱི་མདུན་ལམ་གསར་པ་ཞིག་གི་ལམ་སྟོན་བྱེད་རྒྱུ་བྱུང་།",
  "achievement3_title": "སློབ་གཉེར་ཁང་གི་དུས་དེབ་དང་པོ།",
  "achievement3_desc": "༢༠༡༩ ལོར་ང་ཚོས་ཐར་ལམ་སློབ་གླིང་གི་དུས་དེབ་དང་པོ་དེ་སྤོབས་པ་ཆེན་པོས་པར་སྐྲུན་བྱས། དཔེ་སྐྲུན་འདིས་ང་ཚོའི་སློབ་མ་རྣམས་ཀྱི་རྩོམ་རིག་སྤྱོད་ཡུལ་ཡིན་པ་མ་ཟད། ང་ཚོའི་སློབ་གཉེར་དང་སྤྱི་ཚོགས་ཀྱི་བྱེད་སྒོ་ཁག་གི་ཟིན་ཐོ་གལ་ཆེན་ཞིག་ཀྱང་ཡིན་པས་། ང་ཚོའི་འབྲེལ་མཐུད་དང་དར་སྤེལ་ལ་ལེའུ་གསར་པ་ཞིག་མཚོན་ཡོད།",
  "achievement4_title": "སློབ་གཉེར་ཁང་གི་སྒྲིག་གཞི་དང་དུས་ཚོད་བཅོས་སྒྲིག",
  "achievement4_desc": "༢༠༡༨ ལོར་ང་ཚོས་སློབ་གཉེར་ཁང་གི་སྒྲིག་གཞི་དང་དུས་ཚོད་བཅོས་སྒྲིག་བྱེད་རྒྱུ་བདེ་བླག་ངང་ལག་བསྟར་བྱས། འདིའི་བཅོས་སྒྲིག་དག་ནི་སློབ་གཉེར་གྱི་ཁོར་ཡུག་དང་ཉིན་རེའི་བྱེད་སྒོའི་གྲུབ་ཆ་གོང་འཕེལ་གཏོང་ཆེད་དུ་བཟོས་པ་ཡིན་ལ་། དེ་དག་སྔོན་མའི་གྲ་སྒྲིག་ལས་ལྷག་པའི་ནུས་པ་ལྡན་པ་དང་གསལ་པོ་ཡོང་བའི་ཆེད་དུ་ཡིན།",
  "achievement5_title": "སྨན་ཁང་གི་ཞབས་ཞུ་གོང་འཕེལ།",
  "achievement5_desc": "ང་ཚོའི་སྨན་ཁང་གི་ཞབས་ཞུ་༢༠༢༣ ལོའི་ལོ་འགོར་གལ་ཆེན་པོར་བཅོས་སྒྲིག་བྱས་ཡོད། འདིའི་གོང་འཕེལ་གྱི་ལས་དོན་འདི་ནི་ཐར་ལམ་སློབ་གླིང་གི་སློབ་མ་ཡོངས་ལ་ཐུགས་སྣང་གཡེང་མེད་དང་། འཕྲོད་བསྟེན་ཞབས་ཞུ་ལེགས་ཤོས་འདོན་སྤྲོད་བྱེད་རྒྱུའི་ཆེད་དུ་ཡིན། དེ་ནི་ཁོང་ཚོའི་བདེ་ཐང་སྲུང་སྐྱོབ་དང་སློབ་གཉེར་དང་དགེ་སྒྲུབ་ཀྱི་ལམ་བུར་རྒྱབ་སྐྱོར་བྱེད་པའི་ཆེད་དུ་ཡིན།",
  "facilities_section_title": "ང་ཚོའི་སློབ་གླིང་གི་མཐུན་རྐྱེན་ཁག་མཇལ་རོགས།",
  "facilities_section_subtitle": "ཐར་ལམ་སློབ་གླིང་གིས་དེང་དུས་ཀྱི་མཐུན་རྐྱེན་ཁག་མང་པོ་མཁོ་སྤྲོད་བྱེད་བཞིན་ཡོད་ལ། དེ་དག་ནི་སློབ་མ་རྣམས་ཀྱི་སློབ་གཉེར་དང་། དགེ་སྒྲུབ། ཉིན་རེའི་འཚོ་བ་བཅས་ལ་ཕན་ཐོགས་ཆེད་དུ་བཟོས་པ་ཡིན།",
  "facilities_tab_academic": "སློབ་སྦྱོང་བྱེད་ས།",
