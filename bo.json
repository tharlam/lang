  "event_panel_default_title": "ཞིབ་ཕྲ་མཇལ་རྒྱུའི་བྱེད་སྒོ་ཞིག་འདེམས་རོགས།",
  "event_panel_default_desc": "གོང་གི་བྱེད་སྒོ་རེ་རེ་ནས་གནོན་རོགས། དེ་ཡིས་དེའི་ལོ་རྒྱུས་ཆ་ཚང་དང་། དུས་ཚོད་གལ་ཆེན། དམིགས་བསལ་གྱི་དོན་དག་བཅས་གསལ་པོར་བཟོ་ཡོང་། ང་ཚོའི་བྱེད་སྒོ་ཁག་ནི་སློབ་མའི་འཚོ་བ་གོང་འཕེལ་གཏོང་བ་དང་། སྟོབས་ཆེན་དང་། འབྲེལ་བ་ཟབ་པའི་སྤྱི་ཚོགས་ཤིག་བསྐྲུན་རྒྱུའི་ཆེད་དུ་ཡིན།",
  "health_clinic_hero_title": "ཐར་ལམ་སྨན་ཁང་གི་ཞབས་ཞུ།",
  "health_clinic_services_heading": "ཞབས་ཞུ།",
  "health_clinic_intro_text": "ང་ཚོའི་སྨན་ཁང་གིས་སློབ་མ་དང་། ལས་བྱེད་པ་རྣམས་བདེ་ཐང་ཡོང་ཆེད་ **རྨང་གཞིའི་ཞབས་ཞུ་མཁོ་སྤྲོད་བྱེད་བཞིན་ཡོད་ལ།** དེ་དག་གིས་འཕྲལ་འཕྲལ་གྱི་སྨན་བཅོས་དང་། སྔོན་འགོག་གི་སྨན་བཅོས། བདེ་ཐང་གི་ལས་རིམ་ཁག་བཅས་བདེ་འཇགས་ཀྱི་ཁོར་ཡུག་ཅིག་གི་ནང་དུ་འདོན་སྤྲོད་བྱེད་བཞིན་ཡོད།",
  "health_clinic_wound_care_title": "རྨ་ཁ་སྲུང་སྐྱོབ།",
  "health_clinic_wound_care_desc": "རྨ་ཁ་ཆུང་ཆུང་དང་། གཅོད་ཁ། བརྫད་ཁ་བཅས་ལ་གྲུབ་འབྲས་དང་། གཙང་མ་བཟོ་བ། རྨ་ཐུམ་རྒྱག་པ་བཅས་ཀྱིས་ན་ཚ་མི་ཡོང་བ་དང་། སྨན་བཅོས་ཡོང་བར་བྱེད་པ།",
  "health_clinic_headache_checkup_title": "མགོ་ན་བའི་ཞིབ་བཤེར།",
  "health_clinic_headache_checkup_desc": "མགོ་ན་བའི་ཐོག་མའི་ཞིབ་བཤེར་དང་། བསླབ་བྱ་སྟེར་བ། བདེ་སར་འགོད་པ། སློབ་མ་རྣམས་ལ་ན་ཟུག་གཅོག་རྒྱུའི་གོམ་པ་རྗེས་མའི་ཐད་ལམ་སྟོན་བྱེད་པ།",
  "health_clinic_awareness_title": "འཕྲོད་བསྟེན་སྐོར་གྱི་དན་རྟོགས།",
  "health_clinic_awareness_desc": "འཕྲོད་བསྟེན་དང་། ཟས་བཅུད། ན་ཚ་གཞན་དག བདེ་ཐང་སྤྱི་བཅས་ཀྱི་སྐོར་ལ་ཤེས་ཡོན་གྱི་ལས་རིམ་དང་། དོན་གཉེར་ཁག་གིས་དེ་དག་གི་སྐོར་ལ་མི་མང་ལ་བློ་སྐྱེད་སྟེར་བ།",
  "gallery_main_title": "ང་ཚོའི་ཐར་ལམ་འདྲ་པར་ཁང་།",
  "gallery_description": "ང་ཚོའི་ཡིད་དབང་འཕྲོག་པའི་འདྲ་པར་ཁང་བརྒྱུད་ནས་ཐར་ལམ་སློབ་གླིང་གི་འཚོ་བ་གཟི་བརྗིད་ཅན་དང་། སྲོལ་རྒྱུན་གྱི་རིག་གནས་འབོར་ཆེན་རྣམས་ལ་ཞིབ་འཇུག་བྱེད་རོགས། སློབ་གཉེར་གྱི་གྲུབ་འབྲས་ནས་རིག་གནས་ཀྱི་དུས་ཆེན་བར་དུ་། ང་ཚོའི་ཐུན་མོང་མ་ཡིན་པའི་སྤྱི་ཚོགས་མཚོན་པའི་དུས་སྐབས་ཁག་མཇལ་རོགས།",
  "gallery_building_alt": "ཐར་ལམ་སློབ་གླིང་གི་གཟིམ་ཁང་།",
  "gallery_students_alt": "སློབ་མ་རྣམས།",
  "gallery_view_full_btn": "འདྲ་པར་ཁང་ཆ་ཚང་མཇལ་རོགས།",
  "support_section_title": "ང་ཚོའི་དམིགས་ཡུལ་ལ་རྒྱབ་སྐྱོར་བྱེད་རོགས།",
  "support_section_subtitle": "ཁྱེད་ཀྱི་གཏོང་ཕོད་ཆེ་བའི་རོགས་རམ་གྱིས་ང་ཚོའི་སློབ་མ་རྣམས་ལ་སློབ་གསོ་ལེགས་པོ་དང་། འཚར་ལོངས་ཀྱི་ཁོར་ཡུག་བསྐྲུན་པར་ནུས་པ་སྟེར་གྱི་ཡོད།",
  "support_description_p1": "ཐར་ལམ་སློབ་གླིང་དུ་ང་ཚོ་ཤེས་ཡོན་གྱི་མཐར་ཕྱིན་དང་། བསམ་བློའི་འཕེལ་རྒྱས། སྤྱི་ཚོགས་ཀྱི་རིན་ཐང་བཅས་ལ་ཤུགས་ཆེན་པོ་བརྒྱབ་ཡོད། འོན་ཀྱང་། ང་ཚོའི་མཐུན་རྐྱེན་ཁག་མུ་མཐུད་ནས་སྲུང་སྐྱོབ་བྱེད་རྒྱུ་དང་། སློབ་ཡོན་འདོན་སྤྲོད་བྱེད་རྒྱུ། ཕུན་སུམ་ཚོགས་པའི་ལས་རིམ་ཁག་མཁོ་སྤྲོད་བྱེད་རྒྱུ་བཅས་ལ་ཐོན་ཁུངས་མང་པོ་དགོས་ཀྱི་ཡོད། ཁྱེད་ཀྱི་དཔལ་འབྱོར་གྱི་རྒྱབ་སྐྱོར། ཆེ་ཆུང་གང་ཡིན་རུང་། ང་ཚོའི་སློབ་མའི་མི་ཚེར་ཐད་ཀར་ཤུགས་རྐྱེན་ཐེབས་ཀྱི་ཡོད་ལ། ང་ཚོའི་གལ་ཆེའི་ལས་དོན་མུ་མཐུད་ནས་བྱེད་པར་རོགས་པ་བྱེད་ཀྱི་ཡོད།",
  "support_description_p2": "ང་ཚོ་གསལ་བཤད་དང་། འགན་ཁུར། རོགས་དངུལ་དོ་དམ་བཅས་ལ་ཡིད་ཆེས་བྱེད་ཀྱི་ཡོད། རོགས་དངུལ་རེ་རེ་ང་ཚོའི་སློབ་མར་ཐད་ཀར་ཕན་ཐོགས་པ་དང་། ཁོང་ཚོའི་སློབ་སྦྱོང་གི་ཉམས་མྱོང་གོང་འཕེལ་གཏོང་ཆེད་དུ་ནུས་པ་ལྡན་པར་སྤྱོད་ཀྱི་ཡོད། ཁྱེད་ཀྱི་རྒྱབ་སྐྱོར་ནི་ང་ཚོའི་དམིགས་ཡུལ་མུ་མཐུད་ནས་སྲུང་སྐྱོབ་བྱེད་པར་གལ་ཆེ་བ་ཡིན།",
  "support_cta_text": "གལ་ཏེ་ཁྱེད་ཀྱིས་ཇི་ལྟར་རྒྱབ་སྐྱོར་བྱེད་དགོས་མིན་གྱི་སྐོར་ལ་ཞིབ་ཕྲ་མཁྱེན་འདོད་ཡོད་ན། དམིགས་བསལ་གྱི་རོགས་དངུལ་གཏོང་རྒྱུའི་གོ་སྐབས་བརྗོད་འདོད་ཡོད་ན། ཡང་ན་དཔལ་འབྱོར་གྱི་རྒྱབ་སྐྱོར་དང་འབྲེལ་བའི་དྲི་བ་གང་ཡང་ཡོད་ན། ང་ཚོར་འབྲེལ་གཏུགས་བྱེད་རྒྱུར་སྐུལ་འདེབས་ཞུ་བཞིན་ཡོད། ཐར་ལམ་སློབ་གླིང་གི་མདུན་ལམ་གསར་པ་བསྐྲུན་པར་ཁྱེད་ཀྱི་རྒྱབ་སྐྱོར་ལ་ཐུགས་རྗེ་ཆེ་ཞུ་བཞིན་ཡོད།",
